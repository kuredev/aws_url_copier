{
  "name": "Multi Session URL Copier",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": ["activeTab", "scripting", "clipboardWrite"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    },
    "default_title": "Copy URL"
  }
}
